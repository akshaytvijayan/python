    "#Factorial of a Number\n",
    "a=int(input('Enter The Number: '))\n",
    "i=1\n",
    "f=1\n",
    "while(i<=a):\n",
    "    f=f*i\n",
    "    i=i+1\n",
    "print(\"The Factorial of \",a ,\"is :\",f)"
