   "source": [
    "#39 Count the number of characters(character frequency) in a string\n",
    "a=str(input(\"Enter the String : \"))\n",
    "print()\n",
    "l={}\n",
    "for i in a:\n",
    "    if i in l:\n",
    "        l[i]=l[i]+1\n",
    "    else:\n",
    "        \n",
    "        l[i]=1\n",
    "for key, value in l.items():\n",
    "\n",
    "    print(key,\"\\U0001f600\" ,value)"
   ]
