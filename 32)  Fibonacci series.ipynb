   "source": [
    "#Fibonacci series of n terms\n",
    "a=int(input('Enter The Number: '))\n",
    "print('Fibonacci series : ')\n",
    "First=0\n",
    "Second=1\n",
    "i=1\n",
    "while(i<=a):\n",
    "    print(First)\n",
    "    r=First +Second\n",
    "    First=Second \n",
    "    Second=r\n",
    "    i=i+1"
   ]
